module test;

type Point struct {
    int x;
    char y;
}

func void test1() {
    const int a = 50;
    Point[] p = {
        { 10, 20 },
        { 100, 200 },    // @error{constant value 200 out-of-bounds for type 'int8', range [-128, 127]}
        { 100, 100 + a}, // @error{constant value 150 out-of-bounds for type 'int8', range [-128, 127]}
    }
}

