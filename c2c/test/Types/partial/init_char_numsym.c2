// @warnings{no-unused}
module test;

func void test1() {
    int a = 1;
    char b = 1;
    char c = b + b;
    char d = 100
             + a;   // @warning{implicit conversion loses integer precision: 'int32' to 'int8'}
    char e = 200    // @error{constant value 200 out-of-bounds for type 'int8', range [-128, 127]}
             + a;   // @warning{implicit conversion loses integer precision: 'int32' to 'int8'}
}

